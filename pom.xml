<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>DocumentParserToolkit</groupId>
  <artifactId>DocumentParserToolkit</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>jar</packaging>
  <name>docParser</name>
  <properties>
      <jdk.version>1.7</jdk.version>
  </properties>
  <build>
  <plugins>
	  <plugin>
		<groupId>org.apache.maven.plugins</groupId>
		<artifactId>maven-compiler-plugin</artifactId>
		<version>2.3.2</version>
		<configuration>
			<source>1.7</source>
			<target>1.7</target>
            <showDeprecation>true</showDeprecation>
            <showWarnings>true</showWarnings>
		</configuration>
	  </plugin>
	  <plugin>
		  <groupId>org.apache.maven.plugins</groupId>
		  <artifactId>maven-jar-plugin</artifactId>
		  <version>2.4</version>
		  <configuration>
			<archive>
				<manifest>
					<mainClass>ac.uk.susx.tag.parser.StringDocumentParser</mainClass>
				</manifest>
			</archive>
		  </configuration>
		 </plugin>
	</plugins>
    <sourceDirectory>src</sourceDirectory>
  </build>
  <parent>
  	<version>1.0.3</version>
  	<groupId>uk.ac.susx.tag</groupId>
  	<artifactId>tag-dist</artifactId>
  	<relativePath>./resources/tag-dist</relativePath>
  </parent>
  <dependencies>
      <dependency>
  		<groupId>com.beust</groupId>
  		<artifactId>jcommander</artifactId>
  		<version>1.30</version>
	  </dependency>
	  <dependency>
  		<groupId>org.apache.opennlp</groupId>
  		<artifactId>opennlp-tools</artifactId>
  		<version>1.5.3</version>
	  </dependency>
	  <dependency>
  		<groupId>org.apache.opennlp</groupId>
  		<artifactId>opennlp-maxent</artifactId>
  		<version>3.0.3</version>
	  </dependency>
	  <dependency>
		<groupId>com.google.guava</groupId>
		<artifactId>guava</artifactId>
		<version>15.0</version>
	  </dependency>
	  <dependency>
	    <groupId>berkeleydb</groupId>
	    <artifactId>je</artifactId>
		<version>3.2.76</version>
	  </dependency>
	<dependency>
		<groupId>org.reflections</groupId>
		<artifactId>reflections-maven</artifactId>
		<version>0.9.8</version>
	</dependency>
		  <dependency>
		<groupId>org.apache.commons</groupId>
		<artifactId>commons-io</artifactId>
		<version>1.3.2</version>
	  </dependency>
	  <dependency>
		<groupId>com.googlecode.mate-tools</groupId>
		<artifactId>anna</artifactId>
		<version>3.5</version>
	  </dependency>
	  <dependency>
    	<groupId>info.bliki.wiki</groupId>
    	<artifactId>bliki-core</artifactId>
    	<version>3.0.19</version>
	  </dependency>
	  <dependency>
    	<groupId>org.msgpack</groupId>
    	<artifactId>msgpack</artifactId>
    	<version>0.6.0</version>
  	  </dependency>
  	  <dependency>
		<groupId>com.syncthemall</groupId>
		<artifactId>boilerpipe</artifactId>
		<version>1.2.1</version>
	  </dependency>
	  <dependency>
	  	<groupId>uk.ac.susx.tag</groupId>
	  	<artifactId>dependencyparser</artifactId>
	  	<version>1.17.1</version>
	  </dependency>
	  
  </dependencies>
  <repositories>
      <repository>
        <id>info-bliki-repository</id>
        <url>http://gwtwiki.googlecode.com/svn/maven-repository/</url>
        <releases>
                <enabled>true</enabled>
        </releases>
        <snapshots>
                <enabled>false</enabled>
        </snapshots>
      </repository>
  </repositories>
</project>